{
  "version": 2,
  "builds": [
    {
      "src": "api/efficiency_api.py",
      "use": "@vercel/python"
    },
    {
      "src": "web/**",
      "use": "@vercel/static"
    },
    {
      "src": "landing/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/efficiency_api.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}